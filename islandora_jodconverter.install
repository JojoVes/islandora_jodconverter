/**
 * Implements hook_install().
 *
 * @see islandora_paged_content_islandora_required_objects()
 */
function islandora_jodconverter_install() {
  \Drupal::configFactory()->getEditable('islandora_jodconverter.settings')->set('islandora_jodconverter_openoffice_port', 8100)->save();
}
